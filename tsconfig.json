{
  "extends": "astro/tsconfigs/strict",
  "exclude": ["node_modules/"],
  "compilerOptions": {
    "baseUrl": ".",
    "importsNotUsedAsValues": "error",
    "isolatedModules": false,
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@utils/*": ["src/utils/*"],
      "@typings/*": ["src/typings/*"]
    }
  }
}
